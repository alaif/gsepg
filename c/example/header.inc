//<![CDATA[
<!-- FunkyStuff gets executeed when page is ready -->
function init() {
		$(".program .extended-description").hide();
	//	$("#container").find(".program .name").append("<span class='readbody'>&nbsp;<a href='#' title='Read more'>read more...</a></span>");
		$("#container").find(".program .extended-toggle").append("<span class='readbody'><a href='#' title='Read more'>read more...</a></span>");
		
		
		 $(".program .extended-toggle .readbody a").click(function(event) {
               var mybody = this.parentNode.parentNode;
               while (mybody = mybody.previousSibling) {
                       if (mybody.className == "extended-description") break;
               };
               if (!mybody) return false;
               $(mybody).toggle();
               event.preventDefault();
       });
		
}

<!-- When the page is ready - call FunkyStuff -->
$(document).ready(init);
//]]>
</script>

